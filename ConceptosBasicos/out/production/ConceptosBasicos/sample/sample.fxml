<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.effect.Reflection?>
<?import javafx.scene.effect.SepiaTone?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>


<AnchorPane prefHeight="500.0" prefWidth="500.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Controller">
   <children>
      <Label layoutX="143.0" layoutY="42.0" text="Mi Primer Programa" textFill="#61c999">
         <font>
            <Font name="Book Antiqua Bold" size="24.0" />
         </font>
         <effect>
            <Reflection />
         </effect>
      </Label>
      <TextField fx:id="txtEdad" layoutX="51.0" layoutY="183.0">
         <effect>
            <Reflection bottomOpacity="0.07" fraction="0.52" topOpacity="0.13" />
         </effect>
      </TextField>
      <Label fx:id="lblTitulo" layoutX="51.0" layoutY="156.0" text="Inserta tu edad">
         <font>
            <Font name="Arial" size="12.0" />
         </font>
      </Label>
      <Button layoutX="218.0" layoutY="183.0" mnemonicParsing="false" onAction="#Evento1" text="Aceptar">
         <effect>
            <SepiaTone />
         </effect>
      </Button>
   </children>
</AnchorPane>
